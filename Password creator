import random

Alphabet = ["a","b","c","d",'e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']

CountA  = random.choice (Alphabet)
CountB  = random.choice (Alphabet)
CountC  = random.choice (Alphabet)
CountD  = random.choice (Alphabet)
CountE  = random.choice (Alphabet)
CountF  = random.choice (Alphabet)

password=(CountA+CountB+CountC+CountD+CountE+CountF)
print("your new password is", password)

